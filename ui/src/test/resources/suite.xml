<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite
        name = "Critical Path Suite"
        thread-count = "1"
        configfailurepolicy = "continue"
        verbose = "5">
    <test name = "End-To-End Test">
        <classes>
            <class name = "ru.alfabank.platform.tests.acms.EndToEndTest"/>
            <parameter name = "testPageUri"  value = "about/"/>
            <parameter name = "testWidget"   value = "MetaTitle"/>
            <parameter name = "testProperty" value = "title"/>
        </classes>
    </test>
    <test name = "Search Test">
        <classes>
            <class name = "ru.alfabank.platform.tests.acms.SearchTest"/>
            <parameter name = "testPageUri"  value = "sme-new/"/>
        </classes>
    </test>
</suite>
